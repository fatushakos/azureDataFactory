{
	"name": "DevopsUacPipeline",
	"properties": {
		"activities": [
			{
				"name": "DevopsUacCopyData",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"additionalHeaders": {
							"Content-Type": "application/json"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"quoteAllText": true
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['status']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['id']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_name']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_salary']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_age']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['profile_image']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "RestSamples",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "EmployeesOutputJson",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}